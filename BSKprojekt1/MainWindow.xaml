<Window x:Class="BSKprojekt1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSKprojekt1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ProgressBar HorizontalAlignment="Left" Height="10" Margin="356,309,0,0" VerticalAlignment="Top" Width="100" ValueChanged="ProgressBar_ValueChanged"/>
        <TextBox x:Name="InputFileTextBox" HorizontalAlignment="Left" Height="23" Margin="18,49,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="169"/>
        <Label x:Name="label" Content="Wybierz plik do zaszyfrowania" HorizontalAlignment="Left" Margin="18,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="SelectInputFileButton" Content="Wybierz plik" HorizontalAlignment="Left" Margin="205,52,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.315,0.824" Click="SelectInputFileButton_Click"/>
        <ScrollBar HorizontalAlignment="Left" Margin="605,218,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
